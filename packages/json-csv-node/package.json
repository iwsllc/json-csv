{
  "name": "@iwsio/json-csv-node",
  "version": "6.2.0-alpha.1",
  "description": "ESM/CJS module that easily converts JSON to CSV. This package supports streaming and buffered conversion to CSV.",
  "homepage": "https://github.com/IWSLLC/json-csv",
  "author": "Nathan Bridgewater <info@iws.io>",
  "keywords": [
    "csv",
    "json",
    "esm",
    "node",
    "stream",
    "transform"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/IWSLLC/json-csv.git"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/index.mjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "engines": {
    "node": ">=10.17"
  },
  "scripts": {
    "lint": "eslint src",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "debug": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@iwsio/json-csv-core": "^1"
  },
  "bugs": {
    "url": "https://github.com/IWSLLC/json-csv/issues"
  },
  "license": "BSD-3-Clause"
}
