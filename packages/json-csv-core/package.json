    "json",
    "csv",
    "transform",
    "convert",
    "export csv",
    "convert csv",
    "typescript",
    "export",
    "buffered",
    "browser"
  ],
  "engines": {
    "node": ">=10"
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
