{
  "name": "@iwsio/json-csv-node",
  "version": "6.0.0-alpha.6",
  "description": "Easily convert JSON to CSV in Node.JS. This package supports streaming and buffered conversion to CSV.",
  "homepage": "https://github.com/IWSLLC/json-csv",
  "author": "Nathan Bridgewater <info@iws.io> (https://iws.io/)",
  "keywords": [
    "csv",
    "json",
    "stream",
    "streaming",
    "convert",
    "conversion"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:IWSLLC/json-csv.git"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/index.mjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "engines": {
    "node": ">=10.17"
  },
  "scripts": {
    "prebuild": "rimraf dist && tsc --build --clean tsconfig.build.json",
    "build": "tsc --build tsconfig.build.json"
  },
  "devDependencies": {
    "@types/node": "^18.16.18",
    "debug": "^4.3.2",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-promise": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  },
  "bugs": {
    "url": "https://github.com/IWSLLC/json-csv/issues"
  },
  "dependencies": {
    "@iwsio/json-csv-core": "^1.0.0-alpha.1"
  },
  "license": "BSD-3-Clause"
}
